{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@api/*": [
        "src/api/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    },
    "jsx": "react-native",
    "types": [
      "react",
      "react-native"
    ],
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "extends": "expo/tsconfig.base"
}
